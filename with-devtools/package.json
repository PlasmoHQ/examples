{
  "name": "with-devtools",
  "displayName": "With devtools",
  "version": "0.0.0",
  "description": "A basic Plasmo extension.",
  "author": "lgvic",
  "packageManager": "pnpm@7.5.0",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build"
  },
  "dependencies": {
    "plasmo": "workspace:*",
