{
  "name": "with-messaging",
  "displayName": "With messaging",
  "version": "0.0.1",
  "description": "A basic Plasmo extension.",
  "author": "Plasmo Corp. <foss@plasmo.com>",
  "scripts": {
    "start": "next start",
    "dev": "run-p dev:*",
