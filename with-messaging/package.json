    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@plasmohq/prettier-plugin-sort-imports": "workspace:*",
    "@plasmohq/rps": "workspace:*",
    "@types/chrome": "0.0.233",
    "@types/node": "18.15.12",
    "@types/react": "18.0.37",
    "@types/react-dom": "18.0.11",
    "prettier": "2.8.7",
    "typescript": "5.0.4"
  },
  "manifest": {
    "permissions": [
      "activeTab",
      "tabs"
    ],
    "host_permissions": [
      "https://*/*"
    ]
