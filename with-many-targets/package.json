{
  "name": "with-many-targets",
  "displayName": "With many targets",
  "version": "0.0.0",
  "description": "A basic Plasmo extension.",
  "author": "Plasmo Corp. <foss@plasmo.com>",
  "scripts": {
    "dev": "plasmo dev",
    "dev:firefox": "plasmo dev --target=firefox-mv2",
    "dev:brave": "plasmo dev --target=brave-mv3",
    "build": "run-s build:*",
    "build:chrome": "plasmo build",
    "build:firefox": "plasmo build --target=firefox-mv2",
    "build:brave": "plasmo build --target=brave-mv3"
  },
  "dependencies": {
    "plasmo": "workspace:*",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@plasmohq/prettier-plugin-sort-imports": "workspace:*",
    "@plasmohq/rps": "workspace:*",
    "@types/chrome": "0.0.196",
    "@types/node": "18.7.15",
    "@types/react": "18.0.18",
    "@types/react-dom": "18.0.6",
    "prettier": "2.7.1",
    "typescript": "4.8.2"
  },
  "manifest": {
    "host_permissions": [
      "https://*/*"
    ]
  },
  "contributors": [
    "lgvic"
  ]
}
