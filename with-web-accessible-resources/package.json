{
  "name": "with-web-accessible-resources",
  "displayName": "With web accessible resources",
  "version": "0.0.0",
  "description": "A basic Plasmo extension.",
  "author": "Plasmo Corp. <foss@plasmo.com>",
  "contributors": [
    "louisgv"
  ],
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "plasmo": "workspace:*",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@plasmohq/prettier-plugin-sort-imports": "workspace:*",
    "@types/chrome": "0.0.208",
    "@types/node": "18.11.18",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.10",
    "prettier": "2.8.2",
    "typescript": "4.9.4"
  },
  "manifest": {
    "web_accessible_resources": [
      {
        "resources": [
          "_favicon/*"
        ],
        "matches": [
          "http://localhost:3000/*",
          "https://*.plasmo.com/*"
        ]
      },
      {
        "resources": [
          "~raw.js",
          "assets/pic*.png",
          "resources/test.json"
        ],
        "matches": [
          "https://www.plasmo.com/*"
        ]
      }
    ],
    "host_permissions": [
      "https://*/*"
    ]
  }
}
